msgid "The old password is not valid."
msgstr "رمز عبود قدیمی معتبر نیست."

#, python-brace-format
msgid "Cannot find any account registered with {email}"
msgstr "نمی‌توان حساب‌کاربری ثبت شده‌ای با {email} پیدا کرد."

#, python-brace-format
msgid "Username {username} exists in our database."
msgstr "نام‌کاربری {username} در پایگاه داده ما وجود دارد."

msgid "Unable to login with provided credentials."
msgstr "نمی‌توان با مشخصات داده شده وارد شد."

#, python-brace-format
msgid "Must include \"{username_field}\" and \"password\"."
msgstr "باید \"{username_field}\" و \"رمز عبور\" را وارد کنید."

msgid "User account is disabled."
msgstr "حساب کاربری غیرفعال است."

#, python-brace-format
msgid "Email {email} exists in our database."
msgstr "ایمیل {email} در پایگاه داده ما وجود دارد."

msgid "Template ID does not exists."
msgstr "شناسه قالب وجود ندارد."

msgid "Invalid resend email confirmation."
msgstr "درخواست دوباره فعال سازی غیر معتبر است."

msgid "Invalid Email Verification token."
msgstr "کد فعال سازی ایمیل اشتباه."

msgid "Only images are allowed to be uploaded."
msgstr "تنها عکس‌ها مجاز هستند که آپلود شوند."

msgid "Invitation ID is not valid."
msgstr "مشخه دعوت‌نامه معتبر نیست."

msgid ""
"Login has been disabled on this account for cool down, please try again "
"later."
msgstr ""
"این حساب کاربری برای آرام شدن غیر فعال شده است، لطغا بعدا دوباره امتحان کنید."

msgid "No space left to upload file."
msgstr "فضای کافی برای آپلود فایل وجود ندارد."

msgid "This type of file is not allowed for upload."
msgstr "این نوع فایل برای آپلود مجاز نیست."

msgid "File not found."
msgstr "فایلی یافت نشد."

msgid "Site is required."
msgstr "سایت الزامی است."

msgid "You're not owner of the site."
msgstr "شما صاحب این وبسایت نیستید."

msgid "What?"
msgstr "چی؟"

msgid "It's not possible to promote a user to be an Owner."
msgstr "امکان اینکه کاربر را به صاحب حساب ترقیع داد وجود ندارد."

msgid "Please don't do self promotion."
msgstr "خواهشا سمت خود رو تغییر ندهید."

msgid "This email is already team member."
msgstr "این ایمیل از قبل عضو تیم است."

msgid "Already has been invited."
msgstr "از قبل دعوت شده."

msgid "Cannot find the selected template."
msgstr "نمی‌توان قالب انتخاب شده را پیدا کرد."

#, python-brace-format
msgid "Config field {0} is required."
msgstr "فیلد تنظیمی {0} الزامی است."

#, python-brace-format
msgid "Config field {0} does not exists."
msgstr "فیلید تنظیمی {0} وجود ندارد."

msgid "Unknown team member."
msgstr "عضو ناشناس تیم."

msgid "Website owner cannot be removed from team."
msgstr "صاحب وبسایت نمی‌تواند از تیم حذف شود."

msgid "Sorry, this URL is reserved."
msgstr "شرمنده، این آدرس رزرو شده است."

#, python-brace-format
msgid "Domain {url} already taken."
msgstr "دامنه {url} از قبل گرفته شده."

msgid "Invalid domain."
msgstr "دامنه غیر معنبر."

msgid "Please upload a valid ZIP file."
msgstr "خواهشا یک فایل فشرد ZIP آپلود کنید."

#, python-brace-format
msgid "{0} is missing from ZIP file."
msgstr "{0} در فایل فشرده وجود ندارد."

#, python-brace-format
msgid "{0} (Content Place holder) was not found in  {1}."
msgstr "{0} (نشانه‌گر محتوا) پیدا نشد در  {1}."

msgid "Cannot parse the gonevis.json config"
msgstr "نمی‌توان محتوای gonevis.json را درک کرد"

msgid "Site error conflict!"
msgstr "خطای سایت!"

msgid "You're not owner of this site"
msgstr "شما مالک این سایت نیستید"

msgid "Object Doesn't Exist"
msgstr "شئ وجود ندارد"

msgid "Commenting is not enabled."
msgstr "نظر دادن فعال نیست."

msgid "Your email is not verified."
msgstr "ایمیل شما تایید نشده است."

msgid "Commenting is disabled."
msgstr "نظر دادن غیر‌فعال شده."

msgid "You can't subscribe to your own site."
msgstr "شما نمی توانید وبسایت خود را دنبال کنید."
